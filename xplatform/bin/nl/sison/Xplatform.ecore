<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="xplatform" nsURI="http://www.sison.nl/Xplatform" nsPrefix="xplatform">
  <eClassifiers xsi:type="ecore:EClass" name="Xplatform">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="method" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="uri" eType="#//URI" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="requestHeaders" eType="#//XplatformHeader"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="responseHeaders" eType="#//XplatformHeader"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="jsonToClient" eType="#//XplatformJson"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="jsonToServer" eType="#//XplatformJson"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="URI">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="uriParameter" unique="false"
        upperBound="-1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="XplatformHeader">
    <eStructuralFeatures xsi:type="ecore:EReference" name="headerKeyValues" upperBound="-1"
        eType="#//XplatformHeaderKeyValuePair" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="XplatformHeaderKeyValuePair">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="key" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="XplatformJson">
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//JsonType"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="metaType" eType="#//JsonMetaType"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="composite" eType="#//JsonObject"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonMetaArray">
    <eStructuralFeatures xsi:type="ecore:EReference" name="metaType" eType="#//JsonMetaType"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonMetaType">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="scalar" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="composite" eType="#//JsonMetaArray"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonCompositeType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="composite" eType="#//JsonArray"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="scalar" eType="#//JsonScalarType"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="composite" eType="#//JsonCompositeType"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonScalarType">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="bool" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="string" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="number" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="null" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonArray">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//JsonScalarType" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="composites" upperBound="-1"
        eType="#//JsonCompositeType" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JsonObject">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="keys" unique="false" upperBound="-1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="values" upperBound="-1"
        eType="#//XplatformJson" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
