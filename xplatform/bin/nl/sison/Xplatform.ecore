    <eStructuralFeatures xsi:type="ecore:EReference" name="values" upperBound="-1"
        eType="#//NestedType" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StringList" eSuperTypes="#//NestedType">
